   @click=\"test(item.path)\"\n          link>\n          <!-- v-for 是 vue的标准语法，item in items的 items 是一个数组，:key 表示唯一值 -->\n          <v-list-item-action>\n            <v-icon>{{ item.icon }}</v-icon>\n          </v-list-item-action>\n          <v-list-item-content>\n            <v-list-item-title>\n              {{ item.text }}\n            </v-list-item-title>\n          </v-list-item-content>\n        </v-list-item>\n      </v-list>\n\n    </v-navigation-drawer>\n\n    <!-- v-app-bar 是顶部栏，一般放一些操作类的按钮、搜索功能等 -->\n    <v-app-bar app clipped-left color=\"cyan\" dense>\n      <v-app-bar-nav-icon @click.stop=\"drawer = !drawer\"></v-app-bar-nav-icon>\n      <!-- 此处通过修改drawer变量来决定导航栏是否隐藏，PS：当宽度被压缩的时候导航栏自动隐藏（比如手机） -->\n      <v-toolbar-title></v-toolbar-title>\n      <v-btn text @click = \"reset()\">RESET</v-btn>\n    </v-app-bar>\n\n    <v-main>\n      <router-view></router-view>\n    </v-main>\n\n    <v-footer color=\"cyan\" app>\n      <!-- v-footer是最底部的页脚栏，一般用于放置版权信息之类杂项 -->\n      <v-spacer></v-spacer>\n      <span class=\"white--text\">&copy; {{ new Date().getFullYear() }} Build 0918</span>\n    </v-footer>\n  </v-app>\n</template>\n\n<script>\nexport default {\n  name: \"app\",\n  props: {\n    source: String,\n  },\n  data: () => ({\n    drawer: false,\n    localStorage: window.localStorage,\n    items: [\n      { icon: \"mdi-amplifier\", text: \"Headphone Amps\", path: \"amp\" },\n      { icon: \"mdi-trending-up\", text: \"Loud Speaker Amps\", path: \"charts\" },\n      { icon: \"mdi-alpha-d-circle\", text: \"Digital-to-analog Converter\", path: \"products\" },\n      { icon: \"mdi-account-multiple \", text: \"Power Regenerator\", path: \"vendors\" },      \n      { icon: \"mdi-clock\", text: \"CD Player\", path: \"Earth\" },\n      { icon: \"mdi-history\", text: \"Headphones & IEMs\", path: \"crawler\" },\n    ],\n  }),\n  methods: {\n    test: function (path) {\n      this.$router.push({ name: path, params: {} });\n    },\n    reset: function () {\n      //清除本地Cache\n      this.localStorage.removeItem('state_list')\n      this.localStorage.removeItem('vendor_list')\n      this.localStorage.removeItem('voltage_list')\n      this.localStorage.removeItem('product_list')\n      this.localStorage.removeItem('express_list')\n      this.localStorage.removeItem('price_list')\n      this.localStorage.removeItem('products_class_list')\n    },\n   },\n};\n</script>\n"]}]}